    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  bottom: "pool1_top"
  top: "conv2_top"
  name: "conv2" # conv2_top
  type: "Convolution"
  param{
    name:"conv2_w_"
  }
  param{
    name:"conv2_b_"
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 5
    stride: 2
  }
}
######
layer {
  bottom: "pool1_bottom"
  top: "conv2_bottom"
  name: "conv2_bottom"
  type: "Convolution"
  param{
    name:"conv2_w_"
  }
  param{0
    name:"conv2_b_"
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 5
    stride: 2
  }
}
######
layer {
