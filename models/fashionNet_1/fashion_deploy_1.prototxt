name: "fashionNet_1"

############################ loading data
input: "top"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}

input: "bot"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}

input: "sho"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}
############################ left side (positive) net
layer{
  name: "conv1" # conv1_top
  type: "Convolution"
  bottom: "top"
  top:"conv1_top"
  param{
    name:"conv1_w_"
  }
  param{
    name:"conv1_b_"
  }
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"conv1_bottom"
  type:"Convolution"
  bottom:"bot"
  top:"conv1_bottom"
  param{
    name:"conv1_w_"
  }
  param{
    name:"conv1_b_"
  }
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"conv1_shoes"
  type:"Convolution"
  bottom:"sho"
  top:"conv1_shoes"
  param{
    name:"conv1_w_"
  }
  param{
    name:"conv1_b_"
  }
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"relu1" #relu1_top
  type:"ReLU"
  bottom:"conv1_top"
  top:"conv1_top"
}
######
layer{
  name:"relu1_bottom"
  type:"ReLU"
  bottom:"conv1_bottom"
  top:"conv1_bottom"
}
######
layer{
  name:"relu1_shoes"
  type:"ReLU"
  bottom:"conv1_shoes"
  top:"conv1_shoes"
}
######
layer {
  name: "norm1"
  type: "LRN"
  bottom: "conv1_top"
  top: "norm1_top"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "norm1_bottom"
  type: "LRN"
  bottom: "conv1_bottom"
  top: "norm1_bottom"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "norm1_shoes"
  type: "LRN"
  bottom: "conv1_shoes"
  top: "norm1_shoes"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "norm1_top"
  top: "pool1_top"  
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  name: "pool1_bottom"
  type: "Pooling"
  bottom: "norm1_bottom"
  top: "pool1_bottom"  
