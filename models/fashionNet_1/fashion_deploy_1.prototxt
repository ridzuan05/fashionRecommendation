  top: "metric_fc2_bs"
}
###### triplet interactions on pair-wise interactions
layer {
  name: "metric_fc3_input"
  type: "Concat"
  bottom: "metric_fc2_tb"
  bottom: "metric_fc2_ts"
  bottom: "metric_fc2_bs"
  top: "metric_fc3_input"
  concat_param {
    axis: 1
  }
}
######
layer {
  name: "metric_fc3"
  type: "InnerProduct"
  bottom: "metric_fc3_input"
  top: "metric_fc3"
  param {
    name: "metric_fc3_w"
  }
  param {
    name: "metric_fc3_b"
  }
  inner_product_param {
    num_output: 512
  }   
}
######
layer {
  name: "relu_metric_fc3"
  type: "ReLU"
  bottom: "metric_fc3"
  top: "metric_fc3"
}
######
layer {
  name: "metric_fc4"
  type: "InnerProduct"
  bottom: "metric_fc3"
  top: "metric_fc4"
  param {
    name: "metric_fc4_w"
  }
  param {
    name: "metric_fc4_b"
  }
  inner_product_param {
    num_output: 2
  }   
}
###### soft max to get probability
layer {
  name:   "metric_fc4_softmax"
  type:   "Softmax"
  bottom: "metric_fc4"
  top:    "metric_fc4_softmax"
}
###### slice to get posi_score
layer {
  name:   "slice_posi"
  bottom: "metric_fc4_softmax"
  top:    "dislike"
  top:    "like" # preference score
  type:   "Slice"
  slice_param {
    axis: 1
    slice_point: 1
  }
}