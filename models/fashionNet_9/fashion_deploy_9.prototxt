  }
}
######
#layer {
#  bottom: "fc7_shoes"
#  top: "fc7_shoes_normed"
#  name: "norm7_shoes"
#  type: "Normalization"
#}
#triplet metric net
######
layer {
  name: "outfit_feaVec_tb"
  type: "Concat"
  bottom: "fc7_top"
  bottom: "fc7_bottom"
  top: "outfit_feaVec_tb"
  concat_param {
    axis: 1
  }
}
######
layer {
  name: "outfit_feaVec_ts"
  type: "Concat"
  bottom: "fc7_top"
  bottom: "fc7_shoes"
  top: "outfit_feaVec_ts"
  concat_param {
    axis: 1
  }
}
######
layer {
  name: "outfit_feaVec_bs"
  type: "Concat"
  bottom: "fc7_bottom"
  bottom: "fc7_shoes"
  top: "outfit_feaVec_bs"
  concat_param {
    axis: 1
  }
}
######
layer{
  bottom: "outfit_feaVec_tb"
  top: "metric_fc1_tb"
  name: "metric_fc1_tb"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512
  }    
}
######
layer{
  bottom: "outfit_feaVec_ts"
  top: "metric_fc1_ts"
  name: "metric_fc1_ts"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512
  }    
}
######
layer{
  bottom: "outfit_feaVec_bs"
  top: "metric_fc1_bs"
  name: "metric_fc1_bs"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512
  }    
}
######
layer{
  bottom: "metric_fc1_tb"
  top: "metric_fc2_tb"
  name: "metric_fc2_tb"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512
  }   
}
######
layer{
  bottom: "metric_fc1_ts"
  top: "metric_fc2_ts"
  name: "metric_fc2_ts"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512
  }   
}
######
layer{
  bottom: "metric_fc1_bs"
  top: "metric_fc2_bs"
  name: "metric_fc2_bs"
  type: "InnerProduct"
  inner_product_param {
    num_output: 512 
  }   
}
######
layer {
  name: "metric_relu_tb"
  type: "ReLU"
  bottom: "metric_fc2_tb"
  top: "metric_fc2_tb"
}
######
layer {
  name: "metric_relu_ts"
  type: "ReLU"
  bottom: "metric_fc2_ts"
  top: "metric_fc2_ts"
}
######
layer {
  name: "metric_relu_bs"
  type: "ReLU"
  bottom: "metric_fc2_bs"
  top: "metric_fc2_bs"
}
######
layer {
  name:   "metric_fc3_tb"
  type:   "InnerProduct"
  bottom: "metric_fc2_tb"
  top:    "metric_fc3_tb"
  inner_product_param {
    num_output: 2
  }   
}
######
layer {
  name:   "metric_fc3_ts"
  type:   "InnerProduct"
  bottom: "metric_fc2_ts"
  top:    "metric_fc3_ts"
  inner_product_param {
    num_output: 2
  }   
}
######
layer {
  name:   "metric_fc3_bs"
  type:   "InnerProduct"
  bottom: "metric_fc2_bs"
  top:    "metric_fc3_bs"
  inner_product_param {
    num_output: 2
  }   
}
###### soft max to get probability
layer {
  name:   "metric_fc3_tb_sm"
  type:   "Softmax"
  bottom: "metric_fc3_tb"
  top:    "metric_fc3_tb_sm"
}
###### soft max to get probability
layer {
  name:   "metric_fc3_ts_sm"
  type:   "Softmax"
  bottom: "metric_fc3_ts"
  top:    "metric_fc3_ts_sm"
}
###### soft max to get probability
layer {
  name:   "metric_fc3_bs_sm"
  type:   "Softmax"
  bottom: "metric_fc3_bs"
  top:    "metric_fc3_bs_sm"
}
###### slice to get posi_score
layer {
  name:   "slice_posi_tb"
  bottom: "metric_fc3_tb_sm"
  top:    "dislike_tb"
  top:    "like_tb"
  type:   "Slice"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
###### slice to get posi_score
layer {
  name:   "slice_posi_ts"
  bottom: "metric_fc3_ts_sm"
  top:    "dislike_ts"
  top:    "like_ts"
  type:   "Slice"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
###### slice to get posi_score
layer {
  name:   "slice_posi_bs"
  bottom: "metric_fc3_bs_sm"
  top:    "dislike_bs"
  top:    "like_bs"
  type:   "Slice"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
###### summarize like_scores
layer {
  name:   "like"
  type:   "Eltwise"
  bottom: "like_tb"
  bottom: "like_ts"
  bottom: "like_bs"
  top:    "like"
  eltwise_param{ 
    operation: SUM
  }  
}