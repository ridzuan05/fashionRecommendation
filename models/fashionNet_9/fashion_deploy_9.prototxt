name: "fashionNet_9"

############################ loading data
input: "top"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}

input: "bot"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}

input: "sho"
input_shape {
  dim: 1
  dim: 3
  dim: 224
  dim: 224
}
############################ left side (positive) net
layer {
  name: "conv1" # conv1_top
  type: "Convolution"
  bottom: "top"
  top: "conv1_top"
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"conv1_bottom"
  type:"Convolution"
  bottom:"bot"
  top:"conv1_bottom"
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"conv1_shoes"
  type:"Convolution"
  bottom:"sho"
  top:"conv1_shoes"
  convolution_param{
      num_output: 96
      kernel_size: 7
      stride: 2
  }
}
######
layer{
  name:"relu1" #relu1_top
  type:"ReLU"
  bottom:"conv1_top"
  top:"conv1_top"
}
######
layer{
  name:"relu1_bottom"
  type:"ReLU"
  bottom:"conv1_bottom"
  top:"conv1_bottom"
}
######
layer{
  name:"relu1_shoes"
  type:"ReLU"
  bottom:"conv1_shoes"
  top:"conv1_shoes"
}
######
layer {
  name: "norm1"
  type: "LRN"
  bottom: "conv1_top"
  top: "norm1_top"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "norm1_bottom"
  type: "LRN"
  bottom: "conv1_bottom"
  top: "norm1_bottom"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "norm1_shoes"
  type: "LRN"
  bottom: "conv1_shoes"
  top: "norm1_shoes"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  name: "pool1"
  type: "Pooling"
  bottom: "norm1_top"
  top: "pool1_top"  
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  name: "pool1_bottom"
  type: "Pooling"
  bottom: "norm1_bottom"
  top: "pool1_bottom"  
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  name: "pool1_shoes"
  type: "Pooling"
  bottom: "norm1_shoes"
  top: "pool1_shoes"  
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  bottom: "pool1_top"
  top: "conv2_top"
  name: "conv2" # conv2_top
  type: "Convolution"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 5
    stride: 2
  }
}
######
layer {
  bottom: "pool1_bottom"
  top: "conv2_bottom"
  name: "conv2_bottom"
  type: "Convolution"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 5
    stride: 2
  }
}
######
layer {
  bottom: "pool1_shoes"
  top: "conv2_shoes"
  name: "conv2_shoes"
  type: "Convolution"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 5
    stride: 2
  }
}
######
layer {
  bottom: "conv2_top"
  top: "conv2_top"
  name: "relu2" # relu2_top
  type: "ReLU"
}
######
layer {
  bottom: "conv2_bottom"
  top: "conv2_bottom"
  name: "relu2_bottom"
  type: "ReLU"
}
######
layer {
  bottom: "conv2_shoes"
  top: "conv2_shoes"
  name: "relu2_shoes"
  type: "ReLU"
}
######
layer {
  bottom: "conv2_top"
  top: "norm2_top"
  name: "norm2" # norm2_top
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  bottom: "conv2_bottom"
  top: "norm2_bottom"
  name: "norm2_bottom"
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  bottom: "conv2_shoes"
  top: "norm2_shoes"
  name: "norm2_shoes"
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
######
layer {
  bottom: "norm2_top"
  top: "pool2_top"
  name: "pool2_top"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  bottom: "norm2_bottom"
  top: "pool2_bottom"
  name: "pool2_bottom"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  bottom: "norm2_shoes"
  top: "pool2_shoes"
  name: "pool2_shoes"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
######
layer {
  bottom: "pool2_top"
  top: "conv3_top"
  name: "conv3" #conv3_top
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
######
layer {
  bottom: "pool2_bottom"
  top: "conv3_bottom"
  name: "conv3_bottom"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
######
layer {
  bottom: "pool2_shoes"
  top: "conv3_shoes"
  name: "conv3_shoes"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
######
layer {
  bottom: "conv3_top"
  top: "conv3_top"
  name: "relu3" # relu3_top
  type: "ReLU"
}
######
layer {
  bottom: "conv3_bottom"
  top: "conv3_bottom"
  name: "relu3_bottom"
  type: "ReLU"
}
######
layer {
  bottom: "conv3_shoes"
  top: "conv3_shoes"
  name: "relu3_shoes"
  type: "ReLU"
}
######
layer {
  bottom: "conv3_top"
  top: "conv4_top"
  name: "conv4" # conv4_top
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
######
layer {
  bottom: "conv3_bottom"
  top: "conv4_bottom"
  name: "conv4_bottom"
  type: "Convolution"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
######
layer {
  bottom: "conv3_shoes"
  top: "conv4_shoes"
  name: "conv4_shoes"
  type: "Convolution"
  convolution_param {
